//4. Conversor de Temperatura
//Objetivo: Usar fórmulas matemáticas.
//Enunciado: Converta Celsius para Fahrenheit (F = C * 1.8 + 32).

programa {
    funcao inicio() {
        real celsius
        escreva("Digite a temperatura em Celsius: ")
        leia(celsius)
        escreva("Fahrenheit: ", celsius * 1.8 + 32, "\n")
    }
}


